<!-- BEGIN CONTENT -->
<div class="page-content-wrapper">
    <div class="page-content">

        <!-- BEGIN PAGE CONTENT-->
        <div class="row">

            <div class="col-md-12">

                <h3>My Topologies</h3>

                <% if(topologies.length > 0) { %>
                    <table class="table table-striped table-bordered table-hover" id="topo_configuration">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                                <th>Status</th>
                                <th>Compiled</th>
                                <th>Upload</th>
                                <th>Last update</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <%- partial('topology', { collection: topologies, small: true }) %>
                        </tbody>
                    </table>
                <% } else { %>
                    <p style="margin: 20px 0;">no topology yet. <a href="/editor">Go create one !</a></p>
                <% } %>

            </div>

        </div>

    </div>
    <!-- END PAGE CONTENT-->
</div>
<!-- END CONTENT -->
